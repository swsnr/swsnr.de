source "https://rubygems.org"

gem 'jekyll'

group :jekyll_plugins do
  gem "jekyll-seo-tag"
  gem "jekyll-redirect-from"
  gem "jekyll-feed"
  gem "jekyll-sitemap"
  gem "jekyll-default-layout", git: "https://github.com/benbalter/jekyll-default-layout.git"
  gem 'jekyll-titles-from-headings', git: "https://github.com/benbalter/jekyll-titles-from-headings.git"
  gem "jekyll-relative-links", git: "https://github.com/benbalter/jekyll-relative-links.git"
  gem "jekyll-compose"
  gem "jekyll-commonmark"
end

gem "webrick", "~> 1.7"
